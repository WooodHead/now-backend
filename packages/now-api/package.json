    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^2.3.2",
    "copy-webpack-plugin": "^4.5.2",
    "coveralls": "^3.0.0",
    "eslint": "4.16.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jest": "21.7.0",
    "faker": "git+https://github.com/meetup/faker.js.git#3a4bb358614c1e1f5d73f4df45c13a1a7aa013d7",
    "file": "git+https://github.com/voodoohop/File.git#7fd39e89ea180ccc0d218ee7f8dbd41694275228",
    "flow-bin": "0.64.0",
    "graphql-tag": "^2.9.2",
    "husky": "^0.14.3",
    "jest": "22.1.4",
    "jest-transform-graphql": "^2.1.0",
    "node-fetch": "^2.1.2",
    "nodemon": "^1.17.1",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.13.4",
    "prettier-eslint": "8.8.1",
    "pretty-quick": "^1.4.1",
    "random-emoji": "^1.0.2",
    "rosie": "^2.0.1",
    "vorpal": "^1.12.0",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-node-externals": "^1.6.0"
  },
  "resolutions": {
    "graphql": "0.13.2"
  },
  "jest": {
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/testsetup.js"
  }
}

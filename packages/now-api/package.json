{
  "name": "now-api",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "https://github.com/meetup/mup-web.git",
  "author": "Andy Stanberry <astanberry@meetup.com>",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel server build:watch",
    "server": "NODE_ENV=development AWS_PROFILE=prod AWS_REGION=us-east-1 nodemon dist/server.js localhost 3000",
    "server:debug": "DEBUG=* NODE_ENV=development AWS_PROFILE=prod AWS_REGION=us-east-1 nodemon dist/server.js localhost 3000",
    "stop": "pm2 stop pm2.dev.json",
    "build": "webpack-cli --config webpack.config.dev.js",
    "build:prod": "webpack-cli --config webpack.config.js",
    "build:watch": "webpack-cli --watch  --config webpack.config.dev.js",
    "test": "jest --coverage --runInBand --verbose && eslint . && flow check",
    "test:coverage": "jest --coverage --forceExit --runInBand --verbose",
    "migrate:test": "knex migrate:latest --env test",
    "migrate:development": "knex migrate:latest --env development",
    "migrate:production": "knex migrate:latest --env production",
    "test:watch": "jest --watch --runInBand",
    "lint": "eslint .",
    "flow-start": "flow start",
    "flow-stop": "flow stop",
    "flow-status": "flow status",
    "flow-coverage": "flow coverage",
    "precommit": "pretty-quick --staged",
    "prepush": "eslint .",
    "postinstall": "rm -f node_modules/grpc/node_modules/protobufjs/src/bower.json"
  },
  "dependencies": {
    "apollo-server-express": "^1.3.2",
    "apollo-upload-server": "^5.0.0",
    "aws-sdk": "2.190.0",
    "aws-sns-publish": "^3.2.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dataloader": "^1.4.0",
    "express": "^4.16.2",
    "express-jwt": "^5.3.1",
    "express-jwt-authz": "^1.0.0",
    "firebase-admin": "^5.12.0",
    "form-data": "^2.3.2",
    "graphql": "0.13.2",
    "graphql-joda-types": "^1.0.0",
    "graphql-redis-subscriptions": "^1.5.0",
    "graphql-subscriptions": "^0.5.8",
    "graphql-tools": "3.0.2",
    "graphql-type-json": "^0.2.1",
    "ioredis": "^3.2.2",
    "js-joda": "^1.8.2",
    "js-joda-timezone": "^1.2.1",
    "jwks-rsa": "^1.2.1",
    "knex": "^0.14.6",
    "left-pad": "^1.3.0",
    "lodash": "4.17.5",
    "merge-graphql-schemas": "^1.5.1",
    "morgan": "^1.9.0",
    "pg": "^7.4.3",
    "pm2": "^2.10.1",
    "random-number-csprng": "^1.0.2",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "sharp": "^0.20.1",
    "stream-to-promise": "^2.2.0",
    "subscriptions-transport-ws": "^0.9.6",
    "util.promisify": "^1.0.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "apollo-cache-inmemory": "^1.1.7",
    "apollo-client": "^2.2.3",
    "apollo-link-http": "^1.5.3",
    "apollo-link-schema": "^1.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "8.1.1",

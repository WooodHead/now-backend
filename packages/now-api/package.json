{
  "name": "now-api",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "https://github.com/meetup/mup-web.git",
  "author": "Andy Stanberry <astanberry@meetup.com>",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel build:watch server",
    "server": "NODE_ENV=development AWS_PROFILE=prod AWS_REGION=us-east-1 nodemon -w dist dist/index.js localhost 3000",
    "server:debug": "DEBUG=* server",
    "build": "babel -s true --ignore **/__tests__ src -d dist",
    "build:prod": "babel --env-name production -s true --ignore **/__tests__ src -d dist",
    "build:watch": "babel -w -s true --ignore **/__tests__ src -d dist",
    "test": "jest --coverage --runInBand --verbose && eslint . && flow check",
    "test:coverage": "jest --coverage --forceExit --runInBand --verbose",
    "migrate:test": "knex migrate:latest --env test",
    "migrate:development": "knex migrate:latest --env development",
    "migrate:production": "knex migrate:latest --env production",
    "test:watch": "jest --watch --runInBand",
    "lint": "eslint .",
    "flow-start": "flow start",
    "flow-stop": "flow stop",
    "flow-status": "flow status",
    "flow-coverage": "flow coverage",
    "precommit": "pretty-quick --staged",
    "prepush": "eslint .",
    "apollo:publish": "yarn apollo schema:publish --endpoint=${ENDPOINT} --header=\"Authorization: Bearer ${MACHINE_TOKEN}\" --key=\"${ENGINE_KEY}\"",
    "apollo:publish:local": "ENDPOINT=http://localhost:3000/graphql yarn apollo:publish",
    "apollo:publish:staging": "ENDPOINT=https://now-staging.meetup.com/graphql yarn apollo:publish",
    "apollo:publish:prod": "ENDPOINT=https://now.meetup.com/graphql yarn apollo:publish",
    "fragments": "bin/fragments.js"
  },
  "dependencies": {
    "apollo-engine": "^1.1.2",
    "apollo-server-express": "^2.0.0",
    "apollo-server-module-graphiql": "1.3.2",
    "apollo-upload-server": "^5.0.0",
    "aws-sdk": "2.320.0",
    "aws-sns-publish": "^3.2.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dataloader": "^1.4.0",
    "escape-markdown": "^1.0.4",
    "express": "^4.16.2",
    "express-winston": "^3.0.0",
    "form-data": "^2.3.2",
    "graphql": "0.13.2",
    "graphql-joda-types": "^1.0.0",
    "graphql-postgres-subscriptions": "^1.0.2",
    "graphql-tools": "3.0.2",
    "graphql-type-json": "^0.2.1",
    "js-joda": "^1.8.2",
    "js-joda-timezone": "^1.3.0",
    "jwks-rsa": "^1.2.1",
    "knex": "^0.16.3",
    "left-pad": "^1.3.0",
    "lodash": "4.17.11",
    "merge-graphql-schemas": "^1.5.1",
    "pg": "^7.4.3",
    "random-number-csprng": "^1.0.2",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "sharp": "^0.21.0",
    "source-map-support": "^0.5.9",
    "stream-to-promise": "^2.2.0",
    "subscriptions-transport-ws": "^0.9.15",
    "util.promisify": "^1.0.0",
    "uuid": "^3.2.1",
    "winston": "^3.1.0",
    "winston-cloudwatch": "^2.0.2",
    "wkx": "^0.4.5"
  },
  "devDependencies": {
    "@babel/runtime": "^7.1.2",
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-unicode-property-regex": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-flow": "7.0.0",
    "apollo": "^1.6.0",
    "apollo-cache-inmemory": "^1.1.7",
    "apollo-client": "^2.2.3",
    "apollo-link-http": "^1.5.3",
    "apollo-link-schema": "^1.1.0",
    "apollo-link-ws": "^1.0.9",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-import-graphql": "^2.6.2",
    "chalk": "^2.3.2",
    "coveralls": "^3.0.0",
    "eslint": "4.16.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jest": "21.7.0",
    "faker": "git+https://github.com/meetup/faker.js.git#3a4bb358614c1e1f5d73f4df45c13a1a7aa013d7",
    "file": "git+https://github.com/voodoohop/File.git#7fd39e89ea180ccc0d218ee7f8dbd41694275228",
    "flow-bin": "0.64.0",
    "graphql-tag": "^2.9.2",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "jest-transform-graphql": "^2.1.0",
    "mock-socket-with-protocol": "^7.1.1",
    "node-fetch": "^2.1.2",
    "nodemon": "^1.17.1",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.13.4",
    "prettier-eslint": "8.8.1",
    "pretty-quick": "^1.4.1",
    "random-emoji": "^1.0.2",
    "rosie": "^2.0.1",
    "vorpal": "^1.12.0"
  },
  "resolutions": {
    "graphql": "0.13.2",
    "eslint-scope": "3.7.1"
  },
  "jest": {
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest"
    },
    "rootDir": "src",
    "setupTestFrameworkScriptFile": "<rootDir>/../testsetup.js"
  }
}
